  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6e7781"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#1C868A"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F57A2E"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#F57A2E"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#F57A2E"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#4A85D9"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#C78D3A"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E55E73"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#9333ea"
      }
    },
    {
      "name": "Library/Framework class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#4A85D9"
      }
    },
    {
      "name": "JSON Property Name",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#1C868A"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#1C868A"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#9333ea",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Astro - Component Tag",
      "scope": [
        "entity.name.tag.astro",
        "meta.tag.astro entity.name.tag"
      ],
      "settings": {
        "foreground": "#E55E73",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Astro - Frontmatter Fence",
      "scope": [
        "meta.embedded.fence.astro",
        "punctuation.definition.frontmatter.astro"
      ],
      "settings": {
        "foreground": "#6e7781",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Astro - Props & Astro Globals",
      "scope": [
        "variable.other.object.astro",
        "support.variable.astro"
