{
  "name": "Miku Code",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#c0caf5",
    "activityBar.background": "#16161e",
    "activityBar.foreground": "#7dcfff",
    "activityBar.inactiveForeground": "#3b4261",
    "activityBarBadge.background": "#00d4aa",
    "activityBarBadge.foreground": "#1a1b26",
    "sideBar.background": "#16161e",
    "sideBar.foreground": "#a9b1d6",
    "sideBarTitle.foreground": "#7dcfff",
    "sideBarSectionHeader.background": "#1a1b26",
    "sideBarSectionHeader.foreground": "#7dcfff",
    "tab.activeBackground": "#1a1b26",
    "tab.activeForeground": "#00d4aa",
    "tab.inactiveBackground": "#16161e",
    "tab.inactiveForeground": "#565f89",
    "tab.border": "#1a1b26",
    "editorGroupHeader.tabsBackground": "#16161e",
    "statusBar.background": "#16161e",
    "statusBar.foreground": "#7dcfff",
    "statusBar.noFolderBackground": "#16161e",
    "titleBar.activeBackground": "#16161e",
    "titleBar.activeForeground": "#7dcfff",
    "titleBar.inactiveBackground": "#16161e",
    "titleBar.inactiveForeground": "#565f89",
    "terminal.background": "#1a1b26",
    "terminal.foreground": "#c0caf5",
    "terminal.ansiCyan": "#7dcfff",
    "terminal.ansiGreen": "#00d4aa",
    "terminal.ansiBlue": "#7aa2f7",
    "terminal.ansiMagenta": "#bb9af7",
    "panel.background": "#16161e",
    "panel.border": "#1a1b26",
    "panelTitle.activeForeground": "#7dcfff",
    "panelTitle.inactiveForeground": "#565f89",
    "input.background": "#1a1b26",
    "input.border": "#7dcfff",
    "input.foreground": "#c0caf5",
    "inputOption.activeBackground": "#7dcfff",
    "dropdown.background": "#1a1b26",
    "dropdown.border": "#7dcfff",
    "button.background": "#7dcfff",
    "button.foreground": "#1a1b26",
    "button.hoverBackground": "#00d4aa",
    "scrollbarSlider.background": "#7dcfff40",
    "scrollbarSlider.hoverBackground": "#7dcfff60",
    "scrollbarSlider.activeBackground": "#7dcfff80",
    "editor.selectionBackground": "#7dcfff30",
    "editor.lineHighlightBackground": "#1f2335",
    "editorCursor.foreground": "#00d4aa",
    "editorWhitespace.foreground": "#363b54",
    "editorIndentGuide.background": "#363b5462",
    "editorIndentGuide.activeBackground": "#7dcfff",
    "editorBracketMatch.background": "#7dcfff40",
    "editorBracketMatch.border": "#7dcfff",
    "list.activeSelectionBackground": "#7dcfff30",
    "list.activeSelectionForeground": "#00d4aa",
    "list.inactiveSelectionBackground": "#292e42",
    "list.hoverBackground": "#1f2335",
    "list.focusBackground": "#7dcfff20",
    "peekView.border": "#7dcfff",
    "peekViewEditor.background": "#1a1b26",
    "peekViewResult.background": "#16161e",
    "peekViewTitle.background": "#1a1b26",
    "editorPane.background": "#1a1b26"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#565f89"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "String",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#009d7e"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#c0caf5"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#e0af68"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#f7768e"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Library/Framework class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "JSON Property Name",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#7dcfff"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#00d4aa"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#bb9af7",
        "fontStyle": "bold"
      }
    }
  ]
}
